<div class="pickup__form nested-fields fields-row">
  <div class='form-inline'>
    <div class='input-group date datepicker'>
      <label>Start Date</label>
      <%#= f.text_field :start_date, class: 'form-control', id: 'datepicker1' %>
    </div>
    <div class='input-group date datepicker'>
      <label>End Date</label>
      <%#= f.text_field :end_date, class: 'form-control', id: 'datepicker2' %>
    </div>
  </div>
  <%#= f.input :facilities_list, as: :fake_select, collection: current_shipper.facilities.order(name: :asc), input_html: { class: 'pickup-facility' }, prompt: "自社のリストより撰べます"  %>
  <%#= f.input :company_name %>
  <%= f.input :prefecture, collection: Shipment::PREFECTURES, prompt: "県をお選び下さい" %>
  <%= f.input :ward, collection: Shipment::ADDRESSES, prompt: "住所をお選び下さい" %>
  <%#= f.input :commodity, collection: Carrier::FAVORITE_PRODUCTS, prompt: "荷種をお選び下さい", label: "荷物の種類" %>
  <%#= f.input :weight, collection: ['100', '200', '300', '400', '500', '600', '700'] %>
  <%= link_to_remove_association '集荷先を削除する', f, class: 'btn btnNull btnRemoveNestForm' %>
</div>
