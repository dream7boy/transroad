<% content_for(:after_js) do %>
  <% javascript_tag do %>
    $(function (){
      $('#dummyShipment_post_code').jpostal({
        postcode : [ '#dummyShipment_post_code' ],
        address  : {
          '#dummyShipment_prefecture' : '%3',
          '#dummyShipment_ward'       : '%4'
        }
      });
    });
    $(function (){
      $('#dummyShipment_post_code2').jpostal({
        postcode : [ '#dummyShipment_post_code2' ],
        address  : {
          '#dummyShipment_prefecture2' : '%3',
          '#dummyShipment_ward2'       : '%4'
        }
      });
    });
  <% end %>
<% end %>